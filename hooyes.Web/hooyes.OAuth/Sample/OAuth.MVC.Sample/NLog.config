<?xml version="1.0"?>
<configuration>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="NLog1" xsi:type="File" fileName="${basedir}NLOG.txt"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="NLog1"  />
    </rules>
  </nlog>
</configuration>
