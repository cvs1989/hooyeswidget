    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>billing_log</ID>
          <ToolTip>billing_log</ToolTip>
          <Default>billing_log</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>ID</ID>
          <ToolTip>ID</ToolTip>
          <Default>ID</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>Message</ID>
          <ToolTip>Message</ToolTip>
          <Default>Message</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="sql"><![CDATA[DECLARE @$ID$ int
	   ,@$Message$ nvarchar(4000)
DECLARE SCursor CURSOR FOR
SELECT $ID$,$Message$ FROM $billing_log$  
OPEN SCursor 
    FETCH NEXT FROM SCursor INTO @$ID$,@$Message$
    WHILE (@@FETCH_STATUS = 0)
    BEGIN
    /* TODO */
	PRINT @$ID$
    
    FETCH NEXT FROM SCursor INTO @$ID$,@$Message$
    END
CLOSE SCursor 
DEALLOCATE SCursor]]></Code>
